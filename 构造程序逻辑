"""
寻找水仙花数
"""
for num in range(100,1000):
    x=num%10
    y=num//10%10
    z=num//100
    if x**3+y**3+z**3==num:
        print(num)
        
        
num=int(input('请输入一个三位数：')
      reversed_num=0
      while num>0:
      reversed_num=reversed_num*10+num%10
      num//10
      print(reversed_num)

